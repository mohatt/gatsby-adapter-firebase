      }
    ],
    "ignore": [
      "**/.*",
      "**/node_modules/**",
      "firebase.json"
    ],
    "public": "public",
    "redirects": [
      {
        "destination": "https://www.awesomesite.com/docs/:splat",
        "source": "/docs/:splat*",
        "type": 302
      },
      {
        "destination": "/blog/:splat",
        "source": "/old-blog/:splat*",
        "type": 302
      }
    ],
    "rewrites": [
      {
        "function": {
          "functionId": "test_fetch_info_js"
        },
        "regex": "^/api/test/fetch-info(?:/)?$"
      },
      {
        "function": {
          "functionId": "ssr_engine_cached"
        },
        "source": "/page-data/old-posts/page-data.json"
      },
      {
        "function": {
          "functionId": "ssr_engine",
          "region": "asia-northeast1"
        },
        "source": "/page-data/ssr/page-data.json"
      },
      {
        "function": {
          "functionId": "fetch_info_js"
        },
        "regex": "^/api/fetch-info(?:/)?$"
      },
      {
        "function": {
          "functionId": "hello_world_js"
        },
        "regex": "^/api/hello-world(?:/)?$"
      },
      {
        "function": {
          "functionId": "ssr_engine_cached"
        },
        "regex": "^/old-posts(?:/)?$"
      },
      {
        "function": {
          "functionId": "ssr_engine",
          "region": "asia-northeast1"
        },
        "regex": "^/ssr(?:/)?$"
      }
    ],
    "target": "custom-hosting"
  }
}
