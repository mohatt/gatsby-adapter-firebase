{
  "name": "gatsby-adapter-firebase",
  "version": "1.0.0",
  "description": "Gatsby adapter for Firebase",
  "keywords": [
    "gatsby",
    "transformation"
  ],
  "homepage": "https://github.com/mohatt/gatsby-adapter-firebase",
  "bugs": "https://github.com/mohatt/gatsby-adapter-firebase/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohatt/gatsby-adapter-firebase.git"
  },
  "license": "MIT",
  "author": "Mohamed Elkholy <mohatt@pm.me>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll --clean-dist --target node20",
    "build:watch": "pnpm build --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "prepare": "NODE_ENV=production pnpm build",
    "release": "semantic-release --no-ci",
    "release:inspect": "semantic-release --dry-run",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc -p . --noEmit"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "debug": "^4.4.0",
    "lodash": "^4.17.21",
    "p-limit": "^7.1.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.12",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.19",
    "@vitest/coverage-v8": "^3.2.4",
    "@eslint/js": "^9.37.0",
    "eslint": "^9.37.0",
    "gatsby": "^5.14.6",
    "memfs": "^4.48.1",
    "pkgroll": "^2.17.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "firebase-functions": "^6.0.0",
    "semantic-release": "^24.2.9",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vitest": "^3.2.4",
    "vitest-memfs": "^1.5.1"
  },
  "peerDependencies": {
    "gatsby": ">=5.12.0",
    "firebase-functions": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "firebase-functions": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20"
  },
  "renovate": {
    "extends": [
      "config:recommended"
    ],
    "major": {
      "enabled": false
    }
  }
}
