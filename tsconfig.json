{
  "compilerOptions": {
    "target": "es2022",
    "module": "nodenext",
    "moduleResolution": "node16",
    "lib": ["es2023"],
    "esModuleInterop": true,
    "isolatedModules": true,
    "strict": false,
    "skipLibCheck": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noEmit": true
  },
  "include": ["src", "*.ts"]
}
